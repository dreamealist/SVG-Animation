<script>
var lineDrawing = anime({
  targets: '#animated404 .lines path',
  strokeDashoffset: [anime.setDashoffset, 0],
  easing: 'easeInOutSine',
  duration: 400,
  delay: function(el, i) { return i * 200 },
  direction: 'alternate',
  loop: true
});

var lineDrawing = anime({
  targets: '#animated404 .st2 path',
  strokeDashoffset: [anime.setDashoffset, 0],
  easing: 'easeInOutSine',
  opacity: .0,
  duration: 800,
  delay: function(el, i) { return i * 200 },
  direction: 'alternate',
  loop: true
});
